<% @users.each do |user| %> 
  <li>
    <%= gravatar_for user %>
    <%= link_to user.name, user %>
    <% if current_user.admin? && !user.current_user?(current_user) %>
      | <%= link_to t(".user_delete"), user, method: :delete,
        data: {confirm: t(".user_confirm")} %>
    <% end %>
  </li>
<% end %>
